{
    "configurations": [
        {
            "name": "Fish",
            "includePath": [
                "${workspaceFolder}/FSH",
                "${workspaceFolder}/Libraries/LibC",
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "${workspaceFolder}/Toolchain/Local/bin/x86_64-elf-g++",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-ffreestanding",
                "-fno-rtti",
                "-nostdlib",
                "-nostdinc",
                "-nostdinc++"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/FSH",
                    "${workspaceFolder}/Libraries/LibC",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}
